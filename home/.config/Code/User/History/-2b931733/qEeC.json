{
	// "editor.suggestSelection": "first",
	// "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"window.menuBarVisibility": "toggle",
	"editor.fontSize": 18,
	"editor.fontFamily": "'CartographCF Nerd Font', 'Iosevka Nerd Font', 'JetBrainsMono Nerd Font', 'VictorMono Nerd Font', 'remixicon', 'icomoon\\-feather'",
	"editor.fontWeight": "300",
	"editor.fontLigatures": true,
	"files.autoSave": "onFocusChange",
	"security.workspace.trust.untrustedFiles": "open",
	"editor.bracketPairColorization.enabled": false,
	"editor.cursorSmoothCaretAnimation": "on",
	"files.associations": {
		"dunstrc": "ruby",
		"**/.i3/config": "i3",
		"**/i3/config": "i3",
		"**/i3/**.conf": "i3",
	},
	"editor.formatOnSave": true,
	"diffEditor.codeLens": true,
	"editor.minimap.showSlider": "always",
	"editor.minimap.side": "right",
	"editor.minimap.maxColumn": 100,
	"workbench.colorCustomizations": {
		"[Jellybeans Theme]": {
			// Activity Bar //
			"activityBar.background": "#1c1c1c",
			"activityBar.foreground": "#597bc5",
			"activityBar.inactiveForeground": "#4b4b4b",
			"activityBar.border": "#2e2e2e",
			"activityBar.activeBorder": "#597bc5",
			"activityBarBadge.background": "#597bc5",
			"activityBarBadge.foreground": "#151515",
			// Side Bar //
			"sideBar.background": "#1c1c1c",
			"sideBar.foreground": "#888888",
			"sideBarSectionHeader.background": "#1c1c1c",
			"sideBarSectionHeader.foreground": "#888888",
			"sideBar.border": "#2e2e2e",
			"sideBarSectionHeader.border": "#2e2e2e",
			"sideBarTitle.foreground": "#888888",
			// Breadcrumb //
			"breadcrumb.background": "#1c1c1c",
			"breadcrumb.foreground": "#888888",
			"breadcrumb.focusForeground": "#e8e8d3",
			"breadcrumb.activeSelectionForeground": "#597bc5",
			"breadcrumbPicker.background": "#2e2e2e",
			// Button //
			"button.background": "#597bc5",
			"button.foreground": "#151515",
			// Terminal and Panel //
			"terminal.background": "#1c1c1c",
			"panel.background": "#1c1c1c",
			"panel.border": "#2e2e2e",
			"panelTitle.activeForeground": "#e8e8d3",
			"panelTitle.inactiveForeground": "#888888",
			// Tabs //
			"tab.activeBackground": "#1c1c1c",
			"tab.activeForeground": "#e8e8d3",
			"tab.lastPinnedBorder": "#1c1c1c",
			"editorGroupHeader.tabsBackground": "#1c1c1c",
			"editorGroupHeader.tabsBorder": "#2e2e2e",
			"tab.activeBorder": "#597bc5",
			"tab.border": "#1c1c1c",
			"tab.hoverBackground": "#212121",
			"tab.inactiveBackground": "#1c1c1c",
			"tab.inactiveForeground": "#5b5b5b",
			// Editor //
			"editor.background": "#151515",
			"editorLineNumber.foreground": "#3b3b3b",
			"editorLineNumber.activeForeground": "#888888",
			"editorBracketMatch.background": "#597bc550",
			"editorBracketMatch.border": "#597bc5",
			"editorGutter.background": "#151515",
			// Errors and Warning //
			"editorError.foreground": "#cf6a4c",
			"editorWarning.foreground": "#d8ad4c",
			"editorInfo.foreground": "#71b9f8",
			"editorHint.foreground": "#99ad6a",
			"problemsErrorIcon.foreground": "#cf6a4c",
			"problemsWarningIcon.foreground": "#d8ad4c",
			"problemsInfoIcon.foreground": "#71b9f8",
			//Statusbar
			"statusBar.background": "#1c1c1c",
			"statusBar.foreground": "#888888",
			"statusBar.border": "#2e2e2e",
			"statusBar.noFolderBackground": "#1c1c1c",
			"statusBar.noFolderForeground": "#888888",
			"statusBar.noFolderBorder": "#2e2e2e",
			"statusBar.debuggingBackground": "#1c1c1c",
			"statusBar.debuggingForeground": "#888888",
			"statusBar.debuggingBorder": "#99ad6a",
			// Notifs //
			"notifications.background": "#212121",
			"notifications.foreground": "#e8e8d3",
			"notifications.border": "#2e2e2e",
			// List //
			"list.activeSelectionBackground": "#2e2e2e",
			"list.activeSelectionForeground": "#e8e8d3",
			"list.focusBackground": "#2e2e2e",
			"list.focusForeground": "#e8d8d3",
			"list.focusOutline": "#2e2e2e",
			"list.dropBackground": "#3b3b3b",
			"list.hoverBackground": "#2e2e2e",
			"list.hoverForeground": "#888888",
			"list.errorForeground": "#cf6a4c",
			"list.warningForeground": "#d8ad4c",
			// Git //
			"gitDecoration.modifiedResourceForeground": "#8197bf",
			"gitDecoration.untrackedResourceForeground": "#99ad6a",
		}
	},
	"workbench.colorTheme": "Jellybeans Theme",
	"workbench.iconTheme": "material-icon-theme",
	"open-in-browser.default": "firefox",
	"Lua.telemetry.enable": true,
}